<div class="location" style="display: flex; gap: 20px; padding: 20px; background-color: #f8f9fa;">
    <!-- Office Info Card Container -->
    <div class="card-container" style="flex: 4; height: 100%; overflow-y: auto; box-sizing: border-box;">
        <!-- Office Info Card -->
        <div class="info-card" style="
            padding: 15px; 
            border: none; 
            border-radius: 8px; 
            margin-bottom: 15px; 
            background-color: #ffffff; 
            box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1); ">
            <div class="card-body" style="padding: 20px;">
                <h2 class="card-title" style="
                    font-size: 1.6rem; 
                    margin-bottom: 10px; 
                    color: #00126b; 
                    font-weight: 600;">Carlton Insurance Agency</h2>
                <p class="card-text" style="
                    font-size: 1rem; 
                    line-height: 1.6; 
                    color: #6c757d;">
                    <strong>Address:</strong> 1403 Heritage Drive, Suite B<br>
                    <strong>State:</strong> Minnesota<br>
                    <strong>Zip Code:</strong> 55057<br>
                    <strong>City:</strong> Northfield<br>
                    <strong>Phone:</strong> (612) 808-8900<br>
                    <strong> Website:</strong><a href="https://www.carltonagency.com"> www.carltonagency.com
                    </a>
                </p>
            </div>
        </div>
    </div>
<div class="map-container" style="flex: 60%; border-radius: 10px; overflow: hidden;">
    <iframe 
        src="https://www.google.com/maps/embed/v1/place?key=<%= process.env.GOOGLE_MAPS_API_KEY %>&q=1403+Hertiage+Drive,Suite+B,Northfield,MN,55057" 
        width="100%" 
        height="400" 
        style="border: 0; border-radius: 10px;" 
        allowfullscreen 
        loading="lazy">
    </iframe>
</div>
</div>
